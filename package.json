{
  "name": "digital-business-card",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:workers": "wrangler dev src/workers/index.ts",
    "quick-start": "concurrently -k -n workers,web -c blue,green \"wrangler dev src/workers/index.ts\" \"next dev --turbopack\"",
    "build": "next build",
    "build:workers": "wrangler deploy src/workers/index.ts",
    "start": "next start",
    "lint": "next lint",
    "workers:db:create": "wrangler d1 create script-manager-db",
    "workers:db:migrate": "wrangler d1 migrations apply script-manager-db",
    "workers:db:migrate:local": "wrangler d1 migrations apply script-manager-db --local",
    "workers:r2:create": "wrangler r2 bucket create script-images-dev",
    "workers:kv:create": "wrangler kv namespace create CACHE",
    "deploy:workers:prod": "wrangler deploy src/workers/index.ts --env production",
    "deploy:workers:dev": "wrangler deploy src/workers/index.ts --env development",
    "db:migrate:prod": "wrangler d1 migrations apply script-manager-db --env production",
    "db:migrate:dev": "wrangler d1 migrations apply script-manager-db --env development",
    "setup:prod": "npm run workers:db:create && npm run workers:r2:create && npm run workers:kv:create && npm run db:migrate:prod"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.624.0",
    "@google/generative-ai": "^0.11.3",
    "@hono/zod-validator": "^0.2.0",
    "form-data": "^4.0.4",
    "framer-motion": "^12.10.0",
    "hono": "^4.0.0",
    "lucide-react": "^0.507.0",
    "next": "15.3.2",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240000.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.33.1"
  }
}
